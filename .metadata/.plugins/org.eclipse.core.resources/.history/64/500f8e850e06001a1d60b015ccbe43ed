package com.pojo;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class StatusPOJO {
	private boolean status;

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public StatusPOJO(String status) {
		super();
		this.status = status;
	}
	

}
